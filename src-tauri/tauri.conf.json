{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "TimeUp",
    "version": "0.0.9"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "minimize": true,
        "unminimize": true,
        "startDragging": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "tauri.eykettle.timeup",
      "publisher": "EyKettle",
      "copyright": "Belong to EyKettle.",
      "shortDescription": "Time Up",
      "longDescription": "Time Up",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "customization",
        "decorations": false,
        "transparent": true,
        "fullscreen": false,
        "resizable": false,
        "title": "TimeUp",
        "width": 280,
        "height": 360,
        "minWidth": 280,
        "minHeight": 360,
        "alwaysOnTop": true,
        "skipTaskbar": false
      }
    ]
  }
}
